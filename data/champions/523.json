{"id":523,"name":"残月之肃","alias":"Aphelios","title":"厄斐琉斯","shortBio":"如月影般神出鬼没、时刻剑拔弩张的厄斐琉斯沉默地弑杀一切自身信仰的敌人。他的语言只有精确的瞄准和枪火。虽然他饮下了让他失声的毒药，但他也因此得到了妹妹拉露恩的引导，从遥远的神庙中将月石打造的各种武器送到他手里。只要头顶的明月依然皎洁，厄斐琉斯就永不孤单。","tacticalInfo":{"style":3,"difficulty":3,"damageType":"kPhysical"},"playstyleInfo":{"damage":3,"durability":1,"crowdControl":2,"mobility":1,"utility":1},"squarePortraitPath":"/lol-game-data/assets/v1/champion-icons/523.png","stingerSfxPath":"/lol-game-data/assets/v1/champion-sfx-audios/523.ogg","chooseVoPath":"/lol-game-data/assets/v1/champion-choose-vo/523.ogg","banVoPath":"/lol-game-data/assets/v1/champion-ban-vo/523.ogg","roles":["marksman"],"recommendedItemDefaults":["/DATA/Characters/Aphelios/Recommended/ApheliosARAM.json","/DATA/Characters/Aphelios/Recommended/ApheliosFIRSTBLOOD.json","/DATA/Characters/Aphelios/Recommended/ApheliosKINGPORO.json","/DATA/Characters/Aphelios/Recommended/ApheliosNB.json","/DATA/Characters/Aphelios/Recommended/ApheliosSR.json","/DATA/Characters/Aphelios/Recommended/Beginner.json"],"skins":[{"id":523000,"isBase":true,"name":"残月之肃","splashPath":"/lol-game-data/assets/v1/champion-splashes/523/523000.jpg","uncenteredSplashPath":"/lol-game-data/assets/v1/champion-splashes/uncentered/523/523000.jpg","tilePath":"/lol-game-data/assets/v1/champion-tiles/523/523000.jpg","loadScreenPath":"/lol-game-data/assets/ASSETS/Characters/Aphelios/Skins/Base/ApheliosLoadScreen.Aphelios.jpg","skinType":"","rarity":"kNoRarity","isLegacy":false,"splashVideoPath":null,"collectionSplashVideoPath":null,"featuresText":null,"chromaPath":null,"emblems":null,"regionRarityId":0,"rarityGemPath":null,"skinLines":null,"description":null},{"id":523001,"isBase":false,"name":"黑夜使者 厄斐琉斯","splashPath":"/lol-game-data/assets/v1/champion-splashes/523/523001.jpg","uncenteredSplashPath":"/lol-game-data/assets/v1/champion-splashes/uncentered/523/523001.jpg","tilePath":"/lol-game-data/assets/v1/champion-tiles/523/523001.jpg","loadScreenPath":"/lol-game-data/assets/ASSETS/Characters/A